matrix_n A := 3 3 [[2, 1, 1], [1, 3, 2], [1, 0, 0]];
vector_n b := 3 [4, 5, 6];

// Compute determinant to check if system is solvable
int det_A := det_m A;
if det_A = 0 then {
    print("System has no unique solution");
} else {
    // Compute A^-1
    matrix_f A_inv := inv A;
    
    // Solve x = A^-1 * b
    vector_f x := A_inv mat_mul b;
    print("Solution:");
    print(x);
    
    // Verify solution: A * x should equal b
    vector_f check := A mat_mul x;
    print("Verification:");
    print(check);
}