int a := 5;
float b := 2.5;
float c := a * b;
Print(c);

vector_n v := 3 [1, 2, 3];
vector_f scaled := 2.5 scal_v v;
Print(scaled);

matrix_n m := 2 2 [[1, 2], [3, 4]];
matrix_f m_scaled := 0.5 scal_m m;
Print(m_scaled);

// bool cond := (a > 3) and (b < 5.0); // Try Including this line - Will Raise Error because of boolean Comparison
bool cond := (a > 3) and (a < 10);
Print(cond);