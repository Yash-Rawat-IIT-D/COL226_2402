int y := 10;
{
  int y := 20;
  {
    int y := 45;
    Print(y);
  }
  Print(y);
}
Print(y);

// Advanced Scoping Test

int x := 10;
{
    int x := 20;
    Print(x);
    
    if x > 15 then {
        int y := 30;
        Print(y);
    } else {
        int y := 40;
        Print(y);
    }
}
Print(x);
