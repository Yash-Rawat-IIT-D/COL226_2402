// Test case for boolean expressions

bool a := true and false or not false;
bool b := (true or false) and (not (true and false));
bool c := a and b;

bool d := (true and false) or (false and true) or (true or false);
bool e := not (a or b) and (c or d);

Print(a); Print(b); Print(c); Print(d); Print(e);

// Test Case for int expressions

int a := 50 - 10 * 3 + 4 / 2;  // Expected: 50 - 30 + 2 = 22
int b := (100 + 200) / (5 * 2); // Expected: 300 / 10 = 30
int c := 17 mod 5;              // Expected: 2
int d := (25 mod 7) * 3;        // Expected: (25 mod 7 = 4) * 3 = 12
int e := 10 + 3 * 2 - 8 / 4;    // Expected: 10 + 6 - 2 = 14
int f := (10 - 5) * (4 + 2) / 3; // Expected: (5 * 6) / 3 = 10

Print(a); Print(b); Print(c); Print(d); Print(e); Print(f);

// Test Case for float expressions

float p := 10.5 + 2.5 * 3.0 - 4.0 / 2.0; // Expected: 10.5 + 7.5 - 2.0 = 16.0
float q := (5.0 + 3.0) / (2.0 * 4.0);    // Expected: 8.0 / 8.0 = 1.0
float r := 3.0 * (2.0 + 5.0) / 3.0;      // Expected: 3.0 * 7.0 / 3.0 = 7.0
float s := 1.5 * 4.0 + 2.0 / 0.5;        // Expected: 6.0 + 4.0 = 10.0
float t := 7.5 - 2.5 * 2.0 + 3.0 / 1.5;  // Expected: 7.5 - 5.0 + 2.0 = 4.5

Print(p); Print(q); Print(r); Print(s); Print(t);

