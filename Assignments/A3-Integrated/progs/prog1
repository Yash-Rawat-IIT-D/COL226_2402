matrix_n mat := Input("matrix.txt");

Print(mat);

int rows := dim_v(mat);
int cols := dim_v(mat[0]);
Print(cols);
Print(rows);

matrix_n transpose := cols def_mn rows ;

Print(transpose);

for (int i := 0; i < rows; i := i + 1;) {
    for (int j := 0; j < cols; j := j + 1;) {
        transpose[j][i] := mat[i][j];
    }
}
Print(transpose);